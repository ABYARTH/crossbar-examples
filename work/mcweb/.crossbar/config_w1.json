{
   "workers": [
      {
         "type": "router",
         "options": {
            "cpu_affinity": [0],
            "pythonpath": [".."]
         },
         "transports": [
            {
               "type": "web",
               "endpoint": {
                  "type": "tcp",
                  "port": 8080,
                  "backlog": 2048,
                  "shared": true
               },
               "paths": {
                  "/": {
                     "type": "static",
                     "directory": ".."
                  },
                  "resource": {
                     "type": "resource",
                     "classname": "resource.MyResource"
                  },
                  "json": {
                     "type": "json",
                     "value": {
                        "param1": "foobar",
                        "param2": [1, 2, 3],
                        "param3": {
                           "awesome": true,
                           "nifty": "yes"
                        }
                     }
                  }
               }
            }
         ]
      }
   ]
}
